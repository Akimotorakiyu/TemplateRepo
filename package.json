{
  "name": "template-repo",
  "version": "0.0.0",
  "description": "ğŸ¬ A pnpm template repo, typescript support out of box with vite and jiti.",
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@types/node": "^25.0.10",
    "husky": "^9.1.7",
    "prettier": "^3.8.1",
    "pretty-quick": "^4.2.2",
    "typescript": "5.9.3",
    "vitest": "4.0.18",
    "zx": "^8.8.5"
  },
  "scripts": {
    "prepare": "husky",
    "update-dependencies": "pnpm --recursive update",
    "update-dependencies-latest": "pnpm --recursive update --latest",
    "serve": "pnpm run serve-solidjs",
    "serve-react": "pnpm --filter=./packages/react run serve",
    "serve-solidjs": "pnpm --filter=./packages/solidjs run serve",
    "build": "pnpm --filter=./packages/solidjs run build",
    "dev": "pnpm --filter=./packages/node run dev",
    "test": "vitest --run"
  },
  "keywords": [],
  "author": "Akimotorakiyu",
  "license": "MIT",
  "dependencies": {
    "@commitlint/types": "^20.3.1",
    "lint-staged": "^16.2.7"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
